{
    "solutionName": "GenesisEngine",
    "visualStudioVersion": "2022",
    "libRoot": "ThirdParty/",
    "projectRoot": "%{wks.location}/",
    "defines": [
        "GE_OPENGL",
        "_CRT_SECURE_NO_WARNINGS"
    ],
    "applications": {
        "Client": "bin/%{system}/%{config}/Sandbox-Client/Sandbox-Client.exe",
        "Server": "bin/%{system}/%{config}/Sandbox-Server/Sandbox-Server.exe",
        "Editor": "bin/%{system}/%{config}/Genesis-Editor/Genesis-Editor.exe"
    },
    "projects": [
        {
            "name": "Genesis-Client",
            "location": "Genesis/src/GenesisClient/",
            "includes": "Genesis/src/"
        },
        {
            "name": "Genesis-Server",
            "location": "Genesis/src/GenesisServer/",
            "includes": "Genesis/src/"
        },
        {
            "name": "Genesis-Core",
            "location": "Genesis/src/GenesisCore/",
            "includes": "Genesis/src/"
        },
        {
            "name": "Genesis-ClientCore",
            "location": "Genesis/src/GenesisClientCore/",
            "includes": "Genesis/src/"
        },
        {
            "name": "Sandbox-Server",
            "location": "Sandbox/src/Server/",
            "includes": "Sandbox/src/"
        }
    ],
    "dependencies": [
        {
            "name": "spdlog",
            "packet": {
                "type": "git",
                "repo": "https://github.com/gmasterhd/spdlog"
            },
            "library": {
                "type": "premake",
                "script": "provided",
                "includeDir": "include/"
            }
        },
        {
            "name": "gtest",
            "packet": {
                "type": "git",
                "repo": "https://github.com/gmasterhd/googletest"
            },
            "library": {
                "type": "premake",
                "script": "provided",
                "includeDir": "googletest/include/"
            }
        },
        {
            "name": "boost/preprocessor",
            "packet": {
                "type": "git",
                "repo": "https://github.com/boostorg/preprocessor"
            },
            "library": {
                "type": "header",
                "includeDir": "include/"
            }
        },
        {
            "name": "toml++",
            "packet": {
                "type": "git",
                "repo": "https://github.com/gmasterhd/tomlplusplus"
            },
            "library": {
                "type": "premake",
                "script": "provided",
                "includeDir": "include/"
            }
        },
        {
            "name": "json",
            "group": "nlohmann",
            "packet": {
                "type": "file",
                "name": "json.hpp",
                "path": "include/nlohmann/",
                "url": "https://raw.githubusercontent.com/nlohmann/json/develop/single_include/nlohmann/json.hpp"
            },
            "library": {
                "type": "header",
                "includeDir": "include/"
            }
        },
        {
            "name": "glm",
            "group": "glm",
            "packet": {
                "type": "git",
                "repo": "https://github.com/g-truc/glm"
            },
            "library": {
                "type": "header",
                "includeDir": ""
            }
        },
        {
            "name": "glfw",
            "packet": {
                "type": "git",
                "repo": "https://github.com/GMasterHD/glfw"
            },
            "library": {
                "type": "premake",
                "script": "provided",
                "includeDir": "include/"
            }
        },
        {
            "name": "glad",
            "packet": {
                "type": "git",
                "repo": "https://github.com/GMasterHD/glad"
            },
            "library": {
                "type": "premake",
                "script": "provided",
                "includeDir": "include/"
            }
        },
        {
            "name": "imgui",
            "packet": {
                "type": "git",
                "repo": "https://github.com/ocornut/imgui"
            },
            "library": {
                "type": "premake",
                "script": "imgui.lua",
                "includeDir": ""
            }
        }
    ],
    "links": {
        "Sandbox-Server": [
            "Genesis-Core",
            "Genesis-Server",
            "Sandbox-Server",
            "spdlog",
            "toml++",
            "json",
            "glm",
            "boost/preprocessor"
        ],
        "Sandbox-Client": [
            "Genesis-Core",
            "Genesis-ClientCore",
            "Genesis-Client",
